<div class="modal" id="schematics-examples">
  <div class="modal__container">
    <div class="modal__header">
      <div class="modal__title">Schematics</div>

      <svg class="modal__close">
        <use xlink:href="#close"></use>
      </svg>
    </div>

    <div class="modal__content">
      <div class="modal__schematics row">
        {% for i in range(1, 9) %}
        <div class="col-6 col-lg-3">
          <div class="modal__schematics-item">
            <img src="images/schematics-examples-{{i}}.webp" alt="">
          </div>
        </div>
        {% endfor %}

      </div>
    </div>
  </div>
</div>
