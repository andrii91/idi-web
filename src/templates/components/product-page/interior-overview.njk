<section class="interior-overview">
  <div class="container-xxl">
    <h2 class="interior-overview__title">Interior overview</h2>

    <div class="row interior-overview__gallery">
      {% for i in range(0, 4) %}
      <div class="col-lg-6 interior-overview__item">
        <div class="gallery-thumb" data-index="{{i}}" data-modal="interior">
          <img src="images/interior-{{i}}.webp" alt="">
          <div class="gallery-thumb-icon">
            <svg class="icon">
              <use xlink:href="#upscale-icon"></use>
            </svg>
          </div>
        </div>
      </div>
      {% endfor %}
    </div>
  </div>
</section>

<div class="modal" id="interior">
  <div class="modal__container">
    <div class="modal__header">
      <div class="modal__title">Gallery</div>

      <svg class="modal__close">
        <use xlink:href="#close"></use>
      </svg>
    </div>

    <div class="modal__content">
      <div class="interior-overview__slider">
        {% for i in range(0, 4) %}
          <div class="interior-overview__slider-item">
            <img src="images/interior-{{i}}.webp" alt="">
          </div>
        {% endfor %}

        {% for i in range(1, 6) %}
          <div class="interior-overview__slider-item">
            <img src="images/dinning-room-{{ i }}.webp" alt="dinning-room-{{ i }}">
          </div>
        {% endfor %}

        {% for i in range(1, 6) %}
        <div class="interior-overview__slider-item">
          <img src="images/kitchens-{{ i }}.webp" alt="kitchens-{{ i }}">
        </div>
        {% endfor %}
      </div>

      <div class="interior-overview__slider-nav">
        {% for i in range(0, 4) %}
          <div class="interior-overview__slider-nav-item">
            <img src="images/interior-{{i}}.webp" alt="">
          </div>
        {% endfor %}

        {% for i in range(1, 6) %}
          <div class="interior-overview__slider-nav-item">
            <img src="images/dinning-room-{{ i }}.webp" alt="dinning-room-{{ i }}">
          </div>
        {% endfor %}

        {% for i in range(1, 6) %}
        <div class="interior-overview__slider-nav-item">
          <img src="images/kitchens-{{ i }}.webp" alt="kitchens-{{ i }}">
        </div>
      {% endfor %}
      </div>
    </div>
  </div>
</div>